<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="DemoQA Suite" parallel="false">

    <test name="BrowserStack Chrome">
        <parameter name="runOn" value="BROWSERSTACK"/>
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="os" value="Windows"/>
        <parameter name="osVersion" value="11"/>
        <classes>
            <class name="aitu.sqa.DemoQATextBoxTest"/>
        </classes>
    </test>

    <test name="BrowserStack Firefox">
        <parameter name="runOn" value="BROWSERSTACK"/>
        <parameter name="browser" value="firefox"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="os" value="Windows"/>
        <parameter name="osVersion" value="11"/>
        <classes>
            <class name="aitu.sqa.DemoQATextBoxTest"/>
        </classes>
    </test>

</suite>
